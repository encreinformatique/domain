{
  "name": "encreinformatique/domain",
  "type": "library",
  "description": "Domain handling for PHP applications.",
  "keywords": [
    "domain"
  ],
  "homepage": "http://www.encre-informatique.fr",
  "license": "MIT",
  "authors": [
    {
      "name": "Julien Devergnies",
      "homepage": "http://www.encre-informatique.fr"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "doctrine/orm": "^2.5",
    "doctrine/collections": "^1.3"
  },
  "require-dev": {
    "phpspec/phpspec": "^3.2"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "EncreInformatique\\Component\\Domain\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EncreInformatique\\Component\\Domain\\spec\\": "spec/",
      "EncreInformatique\\Component\\Domain\\Tests\\": "Tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "../../*/*"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
